{
  "builds": [
    {
      "src": "api/rotas2.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "rotas-hardcoded.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(html|css|js|json|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    }
  ]
}
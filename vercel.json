{
  "functions": {
    "api/generate-map.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/generate-map",
      "dest": "/api/generate-map"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(html|css|js|json|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    }
  ]
}